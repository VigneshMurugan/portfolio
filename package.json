{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "-build": "cross-env NODE_OPTIONS=\"-r dotenv/config\" NODE_ENV=production remix build",
    "-dev": "cross-env NODE_OPTIONS=\"-r dotenv/config\" NODE_ENV=development remix dev",
    "build": "npm run build:css && remix build",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles --env production",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "postcss styles/*.css --base styles --dir app/styles -w",
    "postinstall": "remix setup node",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@remix-run/react": "^1.2.3",
    "@remix-run/vercel": "^1.2.3",
    "classnames": "^2.3.1",
    "dotenv": "^16.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.4",
    "eslint": "^8.9.0",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.11",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^7.4.2",
    "prettier": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "vitest": "^0.6.3"
  },
  "engines": {
    "node": ">=14"
  }
}
